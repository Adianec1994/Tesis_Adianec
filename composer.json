{
  "name": "dalholm/laravel-vuetify-spa-admin",
  "description": "A simple lightweight admin template based on laravel and vuetifyjs. Simple and clean",
  "keywords": ["spa", "laravel", "vue", "admin", "template", "frontend", "Material design", "vuetifyjs", "vuetify", "Boilerplate"],
  "license": "MIT",
  "type": "project",
  "authors": [{
    "name": "Mikael Dalholm",
    "email": "mikael@dalholm.se",
    "homepage": "http://www.dalholm.se",
    "role": "Developer"
  }],
  "require": {
    "php": ">=7.1.3",
    "barryvdh/laravel-dompdf": "^0.8.5",
    "doctrine/dbal": "^2.10",
    "fideloper/proxy": "~4.0",
    "fruitcake/laravel-cors": "^1.0",
    "infyomlabs/laravel-generator": "5.7.x-dev",
    "laravel/framework": "5.7.*",
    "laravel/tinker": "~1.0",
    "maatwebsite/excel": "^3.1",
    "spatie/laravel-db-snapshots": "^1.2",
    "spatie/laravel-permission": "^2.37",
    "szymach/c-pchart": "^3.0",
    "tymon/jwt-auth": "dev-develop"
  },
  "require-dev": {
    "filp/whoops": "~2.0",
    "fzaninotto/faker": "~1.4",
    "laravel/dusk": "~3.0",
    "mockery/mockery": "~1.0",
    "phpunit/phpunit": "~7.0",
    "squizlabs/php_codesniffer": "^3.2"
  },
  "autoload": {
    "classmap": [
      "database/seeds",
      "database/factories"
    ],
    "psr-4": {
      "App\\": "app/"
    },
    "files": [
      "app/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "dont-discover": [
        "laravel/dusk"
      ]
    }
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd": [
      "@php artisan key:generate",
      "@php artisan jwt:secret --force"
    ],
    "post-autoload-dump": [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
